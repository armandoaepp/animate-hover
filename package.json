{
  "name": "helper-hover",
  "version": "1.0.1",
  "description": "css classes of help for fronend",
  "main": "gulpfile.js",
  "scripts": {
    "app": "node-sass src/scss/app.scss css/app.css",
    "circle": "node-sass src/scss/circle.scss css/circle.css",
    "square": "node-sass src/scss/square.scss css/square.css",
    "dev": "npm run app & npm run circle & npm run square ",
    "app-min": "node-sass src/scss/app.scss css/app-min.css --output-style compressed",
    "circle-min": "node-sass src/scss/circle.scss css/circle-min.css --output-style compressed",
    "square-min": "node-sass src/scss/square.scss css/square-min.css --output-style compressed",
    "prod": "npm run app-min & npm run circle-min & npm run square-min "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armandoaepp/helper-hover.git"
  },
  "keywords": [
    "helper-hover"
  ],
  "author": "@armandoaepp",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^3.7.0"
  },
  "devDependencies": {
    "node-sass": "^4.9.2"
  }
}
